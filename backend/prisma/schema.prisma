generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Registro {
  id          Int      @id @default(autoincrement())
  horaInicio  DateTime
  horaFim     DateTime
  intervalo   Int
  descricao   String?
  criadoEm    DateTime @default(now())
  atualizadoEm DateTime @updatedAt

  @@index([horaInicio, horaFim])
  @@index([criadoEm])
}
